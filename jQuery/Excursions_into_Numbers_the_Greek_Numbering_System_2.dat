function getGreekNumber( x )
{

    var divisor = 10000; var s=' '; var  s1=''; var i=0;
    var remainder=0; var i=0;
    log('In function getGreekNumber : ', x );
    for (i=0;i<5;i++) {
       var m = Math.floor(x/divisor);
       if (m>=0) {
             s = getNumberString(m,divisor);
       }; 
      remainder = x  %  divisor;
    
      x = remainder;
       s1 = s1 + s;
           
      log('loop :  m,   remainder,  divisor,   html  ' , m ,'   ', remainder, '  ', i, '   ', divisor, '   ', s);

      divisor = divisor/10;
    } 
return s1;

}

