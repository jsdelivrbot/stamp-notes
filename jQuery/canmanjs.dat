 <script type="text/javascript">
Caman("../CodeIgniter/images/angela.jp", "example-canvas", function () {
	this
		.saturation(20)
		.gamma(1.4)
		.vintage()
		.contrast(5)
		.exposure(15)
		.vignette(300, 60)
		.render();
});
</script>        