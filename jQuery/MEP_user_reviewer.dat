$(function() {
		$( "#quecontainer" ).draggable({ opacity: 0.35 });
	});

$(document).ready(function() {

// add a click function to add a user defined 
// activity. This needs to pick up the ID
// from the server and save it automatically
// note the use of val and not value. Damn I always forget this
// and have to google it.

$('#add').click(function(){
     var az= $('#newactivity').val();
     $('#quecontainer :nth-child(3)').html(az);
     // clear the textarea
     $('#newactivity').val('');
   });
});

$(document).ready(function() {
$('#left-menu').css({'visibility':'hidden'});
});

//Options for draggable
var options = {
  revert: true,
  opacity: '0.35'

}
$(function() {
		$( ".activity" ).draggable(options);
	});




